{
    "name": "noeldemartin/laravel-dusk-mocking",
    "description": "Mock facades in Laravel Dusk tests.",
    "keywords": ["laravel", "dusk", "testing"],
    "license": "MIT",
    "authors": [
        {
            "name": "Noel De Martin",
            "email": "noeldemartin@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravel/dusk": "~3.0",
        "illuminate/support": "~5.6",
        "illuminate/contracts": "~5.6",
        "symfony/http-foundation": "~4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "mockery/mockery": "^1.0",
        "fzaninotto/faker": "^1.7"
    },
    "autoload": {
        "psr-4" : {
            "NoelDeMartin\\LaravelDusk\\" : "src/"
        }
    },
    "autoload-dev": {
        "files": [
            "tests/TestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "NoelDeMartin\\LaravelDusk\\MockingServiceProvider"
            ]
        }
    }
}
